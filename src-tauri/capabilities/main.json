{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "main",
    "description": "enables the default permissions",
    "windows": [
      "main"
    ],
"permissions": [

    "dialog:default",
    "dialog:allow-ask",
    "dialog:allow-message",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install",
    "process:allow-restart"
  ]

  }