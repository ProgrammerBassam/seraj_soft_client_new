{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "productName": "seraj-clients",
    "version": "0.14.0",
    "identifier": "com.seraj.clients",
    "build": {
      "frontendDist": "../out",
      "devUrl": "http://localhost:3001",
      "beforeDevCommand": "npm run dev",
      "beforeBuildCommand": "npm run build"
    },
    "app": {
      "windows": [
        {
          "title": "seraj-clients",
          "width": 800,
          "height": 600,
          "resizable": true,
          "fullscreen": false,
          "visible": false
        }
      ],
      "security": {
        "csp": "default-src 'self' https://your-external-source.com; script-src 'self' 'unsafe-inline' 'unsafe-eval';"
      }
    },
    "bundle": {
      "createUpdaterArtifacts": true,
      "active": true,
      "targets": ["msi", "nsis"],
      "category": "Productivity",
      "copyright": "",
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "publisher": "Elijah Lopez",
      "longDescription": "",
      "shortDescription": "seraj clients application",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "plugins": {
      "updater": {
        "active": true,
        "windows": {
          "installMode": "passive"
        },
        "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY5OEQxMTY2MDUyRjExOTYKUldTV0VTOEZaaEdOK2EvOHlBM3d1VFR5T2tiWWFEUFdZYkhFaHVJZDBYMWlxLzMzVDVMeTdVcUsK",
        "endpoints": [
          "https://github.com/ProgrammerBassam/seraj_soft_client_new/releases/latest/download/latest.json"
        ],
        "dialog": true
      },
      "process": {
        "relaunch": true
      },
      "permissions": {
        "updater": {
          "check": true,
          "install": true
        },
        "all": true
      }
    }
  }
  